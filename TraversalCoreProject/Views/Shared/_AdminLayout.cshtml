<!-- beautify ignore:start -->
<html lang="en"
      class="light-style layout-menu-fixed"
      dir="ltr"
      data-theme="theme-default"
      data-assets-path="../assets/"
      data-template="vertical-menu-template-free">
@await Html.PartialAsync("~/Areas/Admin/Views/LayoutPartial/_HeadPartial.cshtml")
<body>
    <!-- Layout wrapper -->
    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
         @await Component.InvokeAsync("_Navbar")
            <!-- Layout container -->
            <div class="layout-page">
                @await Component.InvokeAsync("_Menu")

                <!-- Content wrapper -->
                <div class="content-wrapper">
                  @RenderBody()
                    <!-- / Content -->
                    @await Html.PartialAsync("~/Areas/Admin/Views/LayoutPartial/_FooterPartial.cshtml")
                    <div class="content-backdrop fade"></div>
                </div>
                <!-- Content wrapper -->
            </div>
            <!-- / Layout page -->
        </div>

        <!-- Overlay -->
        <div class="layout-overlay layout-menu-toggle"></div>
    </div>
    @await Html.PartialAsync("~/Areas/Admin/Views/LayoutPartial/_ScriptPartial.cshtml")
</body>
</html>

